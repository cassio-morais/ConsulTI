@model IEnumerable<ConsulTI.Models.FazendaDeGado>

@{
    ViewData["Title"] = "Fazendas de Gado";
}

<h1>@ViewData["Title"] </h1>


<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Nome)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Sexo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UF)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ValorMaximo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ValorMinimo)
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Nome)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Sexo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UF)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ValorMaximo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ValorMinimo)
            </td>
        </tr>
}
    </tbody>
</table>
